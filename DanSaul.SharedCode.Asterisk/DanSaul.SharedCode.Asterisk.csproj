<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>$([System.DateTime]::Now.ToString("yyyy.MM.ddHH.mm"))</Version>
    <Authors>Dan Saul</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AsterNET.NetStandard" Version="2.2.1" />
    <PackageReference Include="AWSSDK.Polly" Version="3.7.102.2" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.101.22" />
    <PackageReference Include="DanSaul.SharedCode.DatabaseSchemas" Version="2022.4.1521.24" />
    <PackageReference Include="DanSaul.SharedCode.Extensions.AmazonS3" Version="2022.4.1315.44" />
    <PackageReference Include="DanSaul.SharedCode.StandardizedEnvironmentVariables" Version="2022.4.1510.12" />
    <PackageReference Include="DanSaul.SharedCode.TTS" Version="2022.4.721.49" />
    <PackageReference Include="Mono.Posix.NETStandard" Version="1.0.0" />
    <PackageReference Include="Npgsql" Version="7.0.0" />
    <PackageReference Include="Serilog" Version="2.12.0" />
    <PackageReference Include="SSH.NET" Version="2020.0.2" />
  </ItemGroup>

</Project>
